<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="per.codeant.common.proxy.UsersMapper">

	<insert id="batchInsert" useGeneratedKeys="true" keyProperty="username" parameterType="java.util.List">
		INSERT INTO users
			 (
				username,
				password,
				enabled
			) VALUES 
		<foreach collection="list" item="record" separator="," index="index">
			(
				#{record.username},
				#{record.password},
				#{record.enabled}
			)
		</foreach>
	</insert>
</mapper>